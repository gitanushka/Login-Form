"import React, { useEffect } from 'react'\r\nimport toast, { Toaster } from 'react-hot-toast';\r\nimport { useFormik } from 'formik';\r\nimport { resetPasswordValidation } from '../helper/validate'\r\nimport { resetPassword } from '../helper/helper'\r\nimport { useAuthStore } from '../store/store';\r\nimport { useNavigate, Navigate } from 'react-router-dom';\r\nimport useFetch from '../hooks/fetch.hook'\r\n\r\nimport styles from '../styles/Username.module.css';\r\n\r\nexport default function Reset() {\r\n\r\n  const { username } = useAuthStore(state => state.auth);\r\n  const navigate = useNavigate();\r\n  const [{ isLoading, apiData, status, serverError }] = useFetch('createResetSession')\r\n\r\n  const formik = useFormik({\r\n    initialValues : {\r\n      password : 'admin@123',\r\n      confirm_pwd: 'admin@123'\r\n    },\r\n    validate : resetPasswordValidation,\r\n    validateOnBlur: false,\r\n    validateOnChange: false,\r\n    onSubmit : async values => {\r\n      \r\n      let resetPromise = resetPassword({ username, password: values.password })\r\n\r\n      toast.promise(resetPromise, {\r\n        loading: 'Updating...',\r\n        success: <b>Reset Successfully...!</b>,\r\n        error : <b>Could not Reset!</b>\r\n      });\r\n\r\n      resetPromise.then(function(){ navigate('/password') })\r\n\r\n    }\r\n  })\r\n\r\n\r\n  if(isLoading) return <h1 className='text-2xl font-bold'>isLoading</h1>;\r\n  if(serverError) return <h1 className='text-xl text-red-500'>{serverError.message}</h1>\r\n  if(status && status !== 201) return <Navigate to={'/password'} replace={true}></Navigate>\r\n\r\n  return (\r\n    <div className=\"container mx-auto\">\r\n\r\n      <Toaster position='top-center' reverseOrder={false}></Toaster>\r\n\r\n      <div className='flex justify-center items-center h-screen'>\r\n        <div className={styles.glass} style={{ width : \"50%\"}}>\r\n\r\n          <div className=\"title flex flex-col items-center\">\r\n            <h4 className='text-5xl font-bold'>Reset</h4>\r\n            <span className='py-4 text-xl w-2/3 text-center text-gray-500'>\r\n              Enter new password.\r\n            </span>\r\n          </div>\r\n\r\n          <form className='py-20' onSubmit={formik.handleSubmit}>\r\n              <div className=\"textbox flex flex-col items-center gap-6\">\r\n                  <input {...formik.getFieldProps('password')} className={styles.textbox} type=\"text\" placeholder='New Password' />\r\n                  <input {...formik.getFieldProps('confirm_pwd')} className={styles.textbox} type=\"text\" placeholder='Repeat Password' />\r\n                  <button className={styles.btn} type='submit'>Reset</button>\r\n              </div>\r\n\r\n          </form>\r\n\r\n        </div>\r\n      </div>\r\n    </div>\r\n  )\r\n}","C:\\Users\\hp\\OneDrive\\Documents\\GitHub\\Login-Form\\login_app\\client\\src\\components\\Recovery.js",[],[],"C:\\Users\\hp\\OneDrive\\Documents\\GitHub\\Login-Form\\login_app\\client\\src\\components\\PageNotFound.js",[],[],"C:\\Users\\hp\\OneDrive\\Documents\\GitHub\\Login-Form\\login_app\\client\\src\\helper\\validate.js",[],["81","82"],"C:\\Users\\hp\\OneDrive\\Documents\\GitHub\\Login-Form\\login_app\\client\\src\\helper\\convert.js",[],[],"C:\\Users\\hp\\OneDrive\\Documents\\GitHub\\Login-Form\\login_app\\client\\src\\helper\\helper.js",[],[],"C:\\Users\\hp\\OneDrive\\Documents\\GitHub\\Login-Form\\login_app\\client\\src\\store\\store.js",[],[],"C:\\Users\\hp\\OneDrive\\Documents\\GitHub\\Login-Form\\login_app\\client\\src\\hooks\\fetch.hook.js",[],[],"C:\\Users\\hp\\OneDrive\\Documents\\GitHub\\Login-Form\\login_app\\client\\src\\middleware\\auth.js",[],[],{"ruleId":"83","severity":1,"message":"84","line":1,"column":17,"nodeType":"85","messageId":"86","endLine":1,"endColumn":26},{"ruleId":"83","severity":1,"message":"87","line":16,"column":23,"nodeType":"85","messageId":"86","endLine":16,"endColumn":30},{"ruleId":"88","severity":1,"message":"89","line":59,"c